{{#if message}}
    <div class="alert alert-danger">{{message}}</div>
{{/if}}
<h1>Add Music</h1>
<form action="/panel/catalog/add" method="post">
    <input type="text" name="itemType" value="Music" readonly><br>
   <input type="text" name="type"placeholder="Type"><br>
   <input type="text" name="title" placeholder="Title"><br>
   <input type="text" name="artist"placeholder="Artist"><br>
   <input type="text" name="label"placeholder="Label"><br>

   <input type="text" name="runTime"placeholder="Run Time"><br>
    <input  type="date" name="releaseDate"placeholder="release Date"><br>

  <button>Add Music</button>
</form>