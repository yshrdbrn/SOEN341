<table>
    <thead>
        <th>Number</th>
        <th>Username</th>
        <th>Email</th>
        <th>Is Admin</th>
    </thead>
    <tbody>
    {{#each users as |user|}}
        <tr>
            <td>{{@index}}</td>
            <td>{{user.username}}</td>
            <td>{{user.email}}</td>
            <td>{{user.isAdmin}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>